<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.toiletissue.declaration.model.dao.DeclarationMapper">


    <resultMap id="declaration" type="com.toiletissue.declaration.model.dto.DeclarationDTO">
        <id property="no" column="review_no"/>
        <result property="id" column="member_id"/>
    </resultMap>

    <insert id="declareReview">
        insert into declaration (review_no,member_id) values (#{no},#{id})
    </insert>


</mapper>